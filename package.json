{
  "name": "@myparcel-eslint/monorepo",
  "version": "0.0.0-alpha.0",
  "private": true,
  "description": "",
  "repository": "github:myparcelnl/eslint",
  "license": "MIT",
  "author": "Edie Lemoine <edie@myparcel.nl>",
  "main": "index.js",
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "build": "nx run-many --target=build --output-style stream --parallel 10",
    "clean": "nx run-many --target=clean --output-style stream --parallel 10",
    "prepare": "is-ci || husky install"
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --fix",
    "package.json": "npx sort-package-json"
  },
  "devDependencies": {
    "@monodeploy/plugin-github": "^0.6.2",
    "@myparcel-eslint/eslint-config-esnext": "workspace:^",
    "@myparcel-eslint/eslint-config-typescript": "workspace:^",
    "@types/eslint": "^8.4.9",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.0.3",
    "monodeploy": "^3.3.1",
    "nx": "^15.0.4",
    "plop": "^3.1.1",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "packageManager": "yarn@3.2.4"
}
